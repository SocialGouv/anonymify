{
  "matchers": {
    "prenom": {
      "corpus": "prenoms.json"
    },
    "nom": {
      "corpus": "noms.json"
    },
    "ville": {
      "corpus": "villes.json"
    },
    "geo": {
      "regexps": ["^\\d\\d?\\.\\d+,\\d\\d?\\.\\d+$"]
    },
    "email": {
      "regexps": ["^[^@/'\";]+@[^@]+\\.[^@]+$"]
    },
    "cp": {
      "regexps": ["^\\d{5}$", "^2a$", "^2b$"]
    },
    "siren": {
      "regexps": ["^\\d{9}$"]
    },
    "siret": {
      "regexps": ["^\\d{14}$"]
    },
    "tel": {
      "regexps": ["^(\\+\\d+\\s?)?0?\\d[:/.\\s]?(\\d{2}[:/.\\s]?){4}$"]
    },
    "ip": {
      "regexps": ["^\\d\\d?\\d?\\.\\d\\d?\\d?\\.\\d\\d?\\d?\\.\\d\\d?\\d?"]
    },
    "json": {
      "regexps": ["^{\\\".*\\\"\\s*}"]
    },
    "adresse": {
      "regexps": [
        "^(\\d+\\s*(bis|ter)?[\\s,]+)?(rond[-\\s]point|impasse|place|avenue|av.?|rue|boulevard|bd.?|chemin)\\s.*$"
      ]
    },
    "date": {
      "regexps": [
        "^\\d\\d?/\\d\\d?/\\d\\d(\\d\\d)?$",
        "^\\d{4}[/-]\\d\\d?[/-]\\d\\d?$"
      ]
    },
    "url": {
      "regexps": ["^https?://.*$"]
    },
    "float": {
      "regexps": ["^\\d+[,.]\\d+$"]
    },
    "integer": {
      "regexps": ["^\\d+$"]
    },
    "civilite": {
      "items": [
        "M",
        "M.",
        "Mr",
        "Mr.",
        "Monsieur",
        "Madame",
        "Mme",
        "Me",
        "Mme.",
        "Mlle."
      ]
    },
    "sexe": {
      "items": ["Homme", "H", "Femme", "F"]
    }
  }
}
