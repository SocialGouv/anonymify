{
  "matchers": {
    "firstname": {
      "corpus": "prenoms"
    },
    "lastname": {
      "corpus": "noms"
    },
    "city": {
      "corpus": "villes"
    },
    "geo": {
      "regexps": ["^\\d\\d?\\.\\d+,\\d\\d?\\.\\d+$"]
    },
    "email": {
      "regexps": ["^[^@/'\";]+@[^@]+\\.[^@]+$"]
    },
    "postcode": {
      "regexps": ["^\\d{5}$", "^2a$", "^2b$"]
    },
    "siren": {
      "regexps": ["^\\d{9}$"]
    },
    "siret": {
      "regexps": ["^\\d{14}$"]
    },
    "phone": {
      "regexps": ["^(\\+\\d+\\s?)?0?\\d[:/.\\s]?(\\d{2}[:/.\\s]?){4}$"]
    },
    "ip": {
      "regexps": ["^\\d\\d?\\d?\\.\\d\\d?\\d?\\.\\d\\d?\\d?\\.\\d\\d?\\d?"]
    },
    "json": {
      "regexps": ["^{\\\".*\\\"\\s*}"]
    },
    "address": {
      "regexps": [
        "^(\\d+\\s*(bis|ter)?[\\s,]+)?(rond[-\\s]point|chemin|route|impasse|place|avenue|av.?|rue|boulevard|bd.?)\\s.*$"
      ]
    },
    "date_fr": {
      "regexps": ["^\\d\\d?/\\d\\d?/\\d\\d(\\d\\d)?$"]
    },
    "date": {
      "regexps": ["^\\d{4}[/-]\\d\\d?[/-]\\d\\d?$"]
    },
    "datetime": {
      "regexps": ["^\\d{4}[/-]\\d\\d?[/-]\\d\\d?T[\\d:.+]+Z?$"]
    },
    "url": {
      "regexps": ["^https?://.*$"]
    },
    "float": {
      "regexps": ["^\\d+[,.]\\d+$"]
    },
    "integer": {
      "regexps": ["^\\d+$"]
    },
    "title": {
      "items": [
        "M",
        "M.",
        "Mr",
        "Mr.",
        "Monsieur",
        "Madame",
        "Mme",
        "Me",
        "Mme.",
        "Mlle."
      ]
    },
    "sex": {
      "items": ["Homme", "H", "Femme", "F"]
    }
  }
}
